{
  "name": "surf-beaches",
  "version": "1.0.0",
  "description": "A surf beaches project",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "start:dev": "ts-node-dev src/index.ts",
    "start:local": "ts-node-dev --transpile-only src/index.ts  | pino-pretty -c",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test": "yarn test:unit && yarn test:functional",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "style:fix": "prettier --write src/**/*.ts test/**/*.ts",
    "style:check": "prettier --check src/**/*.ts test/**/*.ts"
  },
  "engines": {
    "node": "16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anahelenasilva/surf-beaches.git"
  },
  "keywords": [
    "nodejs",
    "api",
    "typescript"
  ],
  "author": "Ana Helena",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anahelenasilva/surf-beaches/issues"
  },
  "homepage": "https://github.com/anahelenasilva/surf-beaches#readme",
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "@types/express": "^4.17.13",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "express-pino-logger": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^6.1.4",
    "pino": "^7.9.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^0.0.40",
    "@types/cors": "^2.8.12",
    "@types/express-pino-logger": "^4.0.3",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/module-alias": "^2.0.1",
    "@types/node": "16",
    "@types/pino": "^7.0.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "jest": "^27.4.5",
    "nock": "^13.2.1",
    "pino-pretty": "^7.6.0",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  }
}
